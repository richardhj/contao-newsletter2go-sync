{
    "name": "richardhj/contao-newsletter2go-sync",
    "type": "contao-module",
    "description": "Synchronize recipients between Contao and Newsletter2Go",
    "keywords": [
        "contao",
        "newsletter",
        "newsletter2go",
        "sync"
    ],
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Richard Henkenjohann",
            "email": "richardhenkenjohann@googlemail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/richardhj/contao-newsletter2go-sync/issues",
        "source": "https://github.com/richardhj/contao-newsletter2go-sync"
    },
    "require": {
        "php": "^5.3 || ^7.0",
        "contao/core": "^3.5.0",
        "codefog/contao-haste": "^4.12",
        "friends-of-contao/contao-associategroups": "~1.3",
        "richardhj/newsletter2go-api": "^1.0.0"
    },
    "autoload": {
        "psr-0": {
            "Newsletter2Go\\": "src/"
        }
    },
    "extra": {
        "contao": {
            "sources": {
                "module": "system/modules/newsletter2go-sync",
                "assets": "assets/newsletter2go-sync"
            }
        }
    }
}